{
    "env": {
      "browser": true
    },
    "extends": "standard",
    "parser": "babel-eslint",
    "rules": {
        "indent": ["error", 4],
        "no-trailing-spaces": 1,
        "curly": 0,
        "eol-last": 0,
        "semi": [2, "always"],
        "space-before-function-paren": 0,
        "no-return-assign": 0,
        "no-cond-assign": 0,
        "comma-dangle": ["error", {
            "arrays": "never",
            "objects": "always-multiline",
            "imports": "always-multiline",
            "exports": "always-multiline",
            "functions": "never"
        }]
    }
  }
